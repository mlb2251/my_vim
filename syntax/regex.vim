syn match char /[a-zA-Z0-9_]/
hi char ctermfg=144
syn match kleene /[*+?|]/
hi kleene ctermfg=166
syn match parens /[()]/
hi parens ctermfg=243
syn match brackets /\[\^\?\|]/
"hi brackets ctermfg=178
hi brackets ctermfg=166
syn match braces /[{}]/
"hi braces ctermfg=11
hi braces ctermfg=166
syn match back /\\./
hi back ctermfg=11
"hi back ctermfg=166


